{
  "name": "balliswine",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "cf:wip": "git commit -m \"[CF-Pages-Skip] WIP\" && git push",
    "check": "astro check && npm run fmt:check && npm run lint",
    "deps": "ncu -i --format group",
    "dev": "concurrently \"astro dev\" \"npm run typewatch\"",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "info": "astro info",
    "lint": "eslint .",
    "lint:debug": "eslint --inspect-config",
    "preview": "astro preview",
    "start": "astro dev",
    "typewatch": "astro check --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.4",
    "@fontsource-variable/noto-emoji": "^5.2.6",
    "@fontsource/geist-mono": "^5.2.6",
    "@fontsource/geist-sans": "^5.2.5",
    "@fontsource/pacifico": "^5.2.6",
    "@tailwindcss/vite": "^4.1.12",
    "astro": "^5.13.5",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lite-youtube-embed": "^0.3.3",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@types/leaflet": "^1.9.20",
    "@types/leaflet.markercluster": "^1.5.5",
    "@typescript-eslint/parser": "^8.41.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-package-json": "^0.56.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "npm-check-updates": "^18.0.3",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "engineStrict": true
}
