{
  "name": "balliswine",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "cf:wip": "git commit -m \"[CF-Pages-Skip] WIP\" && git push",
    "check": "astro check && npm run fmt:check && npm run lint",
    "deps": "ncu -i --format group",
    "dev": "concurrently \"astro dev\" \"npm run typewatch\"",
    "fmt": "prettier . --write",
    "fmt:check": "prettier . --check",
    "info": "astro info",
    "lint": "eslint .",
    "preview": "astro preview",
    "start": "astro dev",
    "typewatch": "astro check --watch --preserveWatchOutput"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@fontsource-variable/noto-emoji": "^5.2.5",
    "@fontsource/geist-mono": "^5.2.6",
    "@fontsource/geist-sans": "^5.2.5",
    "@fontsource/pacifico": "^5.2.5",
    "@tailwindcss/vite": "^4.1.7",
    "astro": "^5.8.0",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lite-youtube-embed": "^0.3.3",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.4.0",
    "@types/leaflet": "^1.9.18",
    "@types/leaflet.markercluster": "^1.5.5",
    "@typescript-eslint/parser": "^8.32.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-package-json": "^0.31.0",
    "eslint-plugin-perfectionist": "^4.13.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.1.0",
    "npm-check-updates": "^18.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript-eslint": "^8.32.1"
  },
  "engines": {
    "node": ">=22 <23"
  },
  "engineStrict": true
}
